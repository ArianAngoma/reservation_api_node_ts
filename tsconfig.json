{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "outDir": "./dist",
    "strict": true,
    "typeRoots": [
      "node_modules/@types",
      "types.d.ts"
    ],
    "types": [
      "node"
    ],
    "inlineSourceMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "useUnknownInCatchVariables": false
  },
  "files": [
    "types.d.ts"
  ]
}
